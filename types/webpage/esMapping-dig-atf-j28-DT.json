	   "settings": {
			"index":{
				"number_of_shards" : 40,
           		"number_of_replicas" : 1
           		},

      		"analysis": {
      
     			 "analyzer": {
     			   "my_folding": {
          				"tokenizer": "standard",
          				"filter": [
            				"lowercase",
            				"asciifolding"
          				]
        			},
        		"shingle_2_4_analyzer": {
          			"type": "custom",
          			"tokenizer": "standard",
          			"filter": [
            			"lowercase",
            			"stop",
            			"asciifolding",
            			"filter_stemmer",
            			"shingle_filter_2_4"
          				]
        		},
        		"shingle_4_analyzer": {
          		"type": "custom",
          		"tokenizer": "standard",
          		"filter": [
            		"lowercase",
            		"stop",
            		"asciifolding",
            		"filter_stemmer",
            		"shingle_filter_4"
          			]
        		},
        		"exact_email_analyzer": {
          			"tokenizer": "uax_url_email",
          			"filter": [
            			"email_splitting_filter_keep_original",
            			"lowercase",
            			"unique"
          				]
        		},
        		"partial_email_analyzer": {
          			"tokenizer": "uax_url_email",
          			"filter": [
            			"email_splitting_filter",
            			"lowercase"
          			],
         			"filter": [
            			"ngram_5_8"
          			]
        		},
        		"partial_phone_analyzer": {
          			"tokenizer": "keyword",
          			"char_filter": [
            			"digits_only"
          				],
          			"filter": [
            			"ngram_3_10"
          			]
        		},
        		"exact_phone_analyzer": {
          			"tokenizer": "keyword",
          			"char_filter": [
            			"digits_only"
          			]
        		}
     		},
      		"filter": {
        		"filter_stemmer": {
          			"type": "porter_stem",
          			"language": "English"
        		},
        		"shingle_filter_2_4": {
          			"type": "shingle",
          			"min_shingle_size": 2,
          			"max_shingle_size": 4,
          			"output_unigrams": false
        			},
        		"shingle_filter_4": {
          			"type": "shingle",
          			"min_shingle_size": 4,
          			"max_shingle_size": 4,
          			"output_unigrams": false
        		},
        		"email_splitting_filter_keep_original": {
          			"type": "pattern_capture",
          			"preserve_original": 1,
          			"patterns": [
            			"(.+)@",
            			"@(.+)"
          			]
        		},
        		"email_splitting_filter": {
          			"type": "pattern_capture",
          			"preserve_original": 0,
          			"patterns": [
            			"(.+)@",
            			"@(.+)"
          			]
        		},
        		"ngram_3_10": {
          			"type": "nGram",
          			"min_gram": 3,
          			"max_gram": 10
        		},
        		"ngram_5_8": {
          			"type": "nGram",
          			"min_gram": 5,
          			"max_gram": 8
        		}
      		},
      		"char_filter": {
        		"digits_only": {
          			"type": "pattern_replace",
          			"pattern": "(\\D)",
          			"replacement": ""
        		}
      		}
    	}
  
